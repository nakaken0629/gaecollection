<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">
  <servlet>
    <servlet-name>GetToken</servlet-name>
    <servlet-class>com.appspot.collabosketch.GetTokenServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddStroke</servlet-name>
    <servlet-class>com.appspot.collabosketch.AddStrokeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InitStrokes</servlet-name>
    <servlet-class>com.appspot.collabosketch.InitStrokesServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NewPicture</servlet-name>
    <servlet-class>com.appspot.collabosketch.NewPictureServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>History</servlet-name>
    <servlet-class>com.appspot.collabosketch.HistoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetToken</servlet-name>
    <url-pattern>/gettoken</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddStroke</servlet-name>
    <url-pattern>/addstroke</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InitStrokes</servlet-name>
    <url-pattern>/initstrokes</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InitStrokes</servlet-name>
    <url-pattern>/initstrokes/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewPicture</servlet-name>
    <url-pattern>/newpicture</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>History</servlet-name>
    <url-pattern>/history</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
